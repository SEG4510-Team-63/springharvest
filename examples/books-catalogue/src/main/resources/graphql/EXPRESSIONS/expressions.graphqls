input StringExpression {
    equals: String
    contains: String
    in: [String!]
    starts: String
    ends: String
}

input IntExpression {
    eq: Int
    gt: Int
    gte: Int
    lt: Int
    lte: Int
    in: [Int!]
}

input DateExpression {
    eq: Date
    gt: Date
    gte: Date
    lt: Date
    lte: Date
    between: [Date!]
}
extend type Query
{
    searchPets(filter: PetFilter = {}, paging: PageRequest = {page: 0, size: 20, sortOrders: "name", sortDirection: "D"}) : [Pet!]
    searchPetsSimple(filter: PetFilter = {}) : [Pet!]
}

type Pet implements BaseEntity
{
    id: ID!
    name: String!
    traceData: TraceData
}

input PetFilter{
    id: IdExpression
    name: StringExpression
    traceData: TraceDataFilter

    and: [PetFilter!]
    or: [PetFilter!]
    distinct: [PetFilter!]
    not: PetFilter
}


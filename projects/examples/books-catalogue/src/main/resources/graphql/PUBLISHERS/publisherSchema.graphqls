extend type Query {
    searchPublishers(filter: PublisherFilter = {}, paging: PageRequest = {page: 0, size: 20, sortOrders: "name", sortDirection: "D"}): [Publisher!]
}

type Publisher implements BaseEntity{
    id: ID!
    name: String!
    traceData: TraceData
}

input PublisherFilter{
    id: IdExpression
    name: StringExpression
    traceData: TraceDataFilter

    and: [PublisherFilter!]
    or: [PublisherFilter!]
    distinct: [PublisherFilter!]
    not: PublisherFilter
}